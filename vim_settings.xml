        <text>def webhook(id):
    if (id == app.config.get('SECRET_KEY')):
        params = get_params(os.path.join(app.config['APP_ROOT'], 'config/site.ini'), 'get')
        if request.is_json:
            packet = request.get_json()
            app.logger.info(packet)
            uid = validate_uid(packet['data']['uuid'])

        if uid is not None:
            url = params['url'] + '?id=' + uid
            app.logger.info(&quot;URL is {}&quot;.format(url))
            data = (fetch_data_from_url(url, params['user'], params['password'])).json()

            if '#edited' in data['tags']['keyword']:
                store_uid(uid)
                data['revision'] = '1200'
            else:
                data['revision'] = '2'

            images = []
            if data['relationships']['child'] is not None:
                children = data['relationships']['child']
                for child in children:
                    if child['asset_type'] == 'image':
                        url = params['url'] + '?id=' + child['id']
                        image_filename = fetch_child_images(url, **{
                            'user': params['user'],
                            'password': params['password'],
                            'filename': &quot;{}/{}.jpg&quot;.format(os.path.join(app.config['APP_ROOT'], 'images'), child['id'])
                        })
                        images.append(image_filename)

            xml = etree.ElementTree(create_xml(data))
            filename = &quot;{}/{}.xml&quot;.format(os.path.join(app.config['APP_ROOT'], 'xmls'), data['id'])
            xml.write(filename, encoding='utf-8', pretty_print=True,
                    method='xml')
            binary_data = create_zip({
                'xml': filename,
                'images': images,
            })
            app.logger.info(&quot;Sending to NG&quot;)
            val = get_params(os.path.join(app.config['APP_ROOT'], 'config/site.ini'), 'createupdate_test')
            result = call_NG3_importUpdate(val['url'], binary_data)
            app.logger.info(str(xmltodict.parse(result)))
            return 'OK', 200

    return 'Invalid ID', 200</text>
